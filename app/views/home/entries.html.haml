.container
  %ol.breadcrumb
    %li
      = link_to 'Home', root_path
    - if @entry_list.current_page == 1
      %li.active Entries
    - else
      %li Entries
      %li.active= @entry_list.current_page
.container
  .text-center
    = paginate @entry_list
  %ul.list-group
    - @entry_list.each do |entry|
      %li.list-group-item
        = link_to entry.title, entry.url, target: '_blank'
        %small
          \-
          = link_to entry.feed.title, entry.feed.url
  .text-center
    = paginate @entry_list
